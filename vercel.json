{
  "version": 2,
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "^/static/(.*)",
      "dest": "/frontend/public/static/$1"
    },
    {
      "src": "^/(login|home|dashboard|packages|borrow|return|history)/?$",
      "dest": "/frontend/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}